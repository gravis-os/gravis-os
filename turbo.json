{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "yalc:push": {
      "dependsOn": ["build"],
      "cache": false
    },
    "watch": {
      "dependsOn": ["yalc:push"],
      "cache": false
    },
    "tslint": {}
  }
}
