{
  "name": "@gravis-os/apps",
  "version": "1.3.0",
  "sideEffects": false,
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "accounting": [
        "./dist/types/accounting/index.d.ts"
      ],
      "calendar": [
        "./dist/types/calendar/index.d.ts"
      ],
      "directory": [
        "./dist/types/directory/index.d.ts"
      ],
      "blog": [
        "./dist/types/blog/index.d.ts"
      ],
      "ads": [
        "./dist/types/ads/index.d.ts"
      ],
      "tags": [
        "./dist/types/tags/index.d.ts"
      ],
      "categories": [
        "./dist/types/categories/index.d.ts"
      ],
      "forum": [
        "./dist/types/forum/index.d.ts"
      ],
      "crm": [
        "./dist/types/crm/index.d.ts"
      ],
      "pos": [
        "./dist/types/pos/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./accounting": {
      "types": "./dist/types/accounting/index.d.ts",
      "require": "./dist/cjs/accounting/index.js",
      "import": "./dist/esm/accounting/index.js"
    },
    "./calendar": {
      "types": "./dist/types/calendar/index.d.ts",
      "require": "./dist/cjs/calendar/index.js",
      "import": "./dist/esm/calendar/index.js"
    },
    "./directory": {
      "types": "./dist/types/directory/index.d.ts",
      "require": "./dist/cjs/directory/index.js",
      "import": "./dist/esm/directory/index.js"
    },
    "./blog": {
      "types": "./dist/types/blog/index.d.ts",
      "require": "./dist/cjs/blog/index.js",
      "import": "./dist/esm/blog/index.js"
    },
    "./ads": {
      "types": "./dist/types/ads/index.d.ts",
      "require": "./dist/cjs/ads/index.js",
      "import": "./dist/esm/ads/index.js"
    },
    "./tags": {
      "types": "./dist/types/tags/index.d.ts",
      "require": "./dist/cjs/tags/index.js",
      "import": "./dist/esm/tags/index.js"
    },
    "./categories": {
      "types": "./dist/types/categories/index.d.ts",
      "require": "./dist/cjs/categories/index.js",
      "import": "./dist/esm/categories/index.js"
    },
    "./forum": {
      "types": "./dist/types/forum/index.d.ts",
      "require": "./dist/cjs/forum/index.js",
      "import": "./dist/esm/forum/index.js"
    },
    "./crm": {
      "types": "./dist/types/crm/index.d.ts",
      "require": "./dist/cjs/crm/index.js",
      "import": "./dist/esm/crm/index.js"
    },
    "./pos": {
      "types": "./dist/types/pos/index.d.ts",
      "require": "./dist/cjs/pos/index.js",
      "import": "./dist/esm/pos/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "description": "Powered by One X Tech",
  "author": "One X Tech",
  "license": "GPL-3.0",
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "watch": "tsc-watch -p tsconfig.esm.json --onSuccess \"npx yalc push\"",
    "reset": "rm -rf dist && rm -rf node_modules && npx yalc remove --all",
    "yalc:push": "yalc push",
    "test": "jest",
    "lint": "eslint --ext .ts,.js,.jsx,.tsx . --fix --quiet",
    "tslint": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "@gravis-os/crud": "*",
