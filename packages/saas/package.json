{
  "name": "@gravis-os/saas",
  "version": "0.3.0",
  "sideEffects": false,
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "description": "Powered by One X Tech",
  "author": "One X Tech",
  "license": "GPL-3.0",
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "watch": "tsc-watch -p tsconfig.esm.json --onSuccess \"npm run yalc:push\"",
    "reset": "rm -rf dist && rm -rf node_modules && npx yalc remove --all",
    "yalc:push": "yalc push",
    "lint": "lint-staged && next lint --quiet --fix",
    "tslint": "tsc --noEmit --incremental"
  },
  "peerDependencies": {
    "@supabase/auth-helpers-nextjs": ">=0.2.8",
    "@supabase/auth-helpers-react": ">=0.2.4",
    "@supabase/supabase-js": ">=1.35.6",
    "next": ">=12.1.5",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-query": ">=3.39.2"
  },
  "dependencies": {
    "@gravis-os/middleware": "*",
    "@gravis-os/types": "*",
    "@gravis-os/utils": "*"
  },
  "devDependencies": {
    "@gravis-os/eslint-config": "*",
    "@gravis-os/tsconfig": "*",
    "@supabase/auth-helpers-nextjs": "^0.2.5",
    "@supabase/auth-helpers-react": "^0.2.3",
    "@supabase/supabase-js": "^1.35.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.15.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "next": "^12.1.5",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "ts-loader": "^8.0.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.68.0",
    "yalc": "^1.0.0-pre.53"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --quiet --fix"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
